{
	"name": "app-esa-world-cereal-product-viewer",
	"description": "ESA World Cereal: Product Viewer",
	"keywords": [
		"panther",
		"gisat",
		"ptr",
		"worldCereal",
		"esa"
	],
	"path": "/",
	"version": "0.0.1",
	"private": true,
	"prettier": "@gisatcz/prettier-config",
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/gisat-panther/app-worldWaterPreliminaryDataReview"
	},
	"dependencies": {
		"@gisatcz/ptr-atoms": "^1.3.13",
		"@gisatcz/ptr-charts": "^1.3.4",
		"@gisatcz/ptr-components": "^1.2.4",
		"@gisatcz/ptr-core": "^1.4.0",
		"@gisatcz/ptr-maps": "^1.7.0-dev.0",
		"@gisatcz/ptr-router": "^0.0.4",
		"@gisatcz/ptr-state": "2.0.0-dev.17",
		"@gisatcz/ptr-utils": "^1.4.0-dev.0",
		"classnames": "^2.3.1",
		"history": "^5.0.1",
		"isomorphic-fetch": "^3.0.0",
		"lodash": "^4.17.21",
		"prop-types": "^15.7.2",
		"re-reselect": "^4.0.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-helmet": "^6.1.0",
		"react-loadable": "^5.5.0",
		"react-scripts": "^4.0.3",
		"react-uid": "^2.3.1",
		"url-loader": "^4.1.1"
	},
	"devDependencies": {
		"@cra-express/core": "^4.3.2",
		"@gisatcz/prettier-config": "0.0.3",
		"cra-universal": "^4.5.0",
		"node-sass": "^4.11.0",
		"husky": "^7.0.2",
		"local-cors-proxy": "^1.1.0",
		"prettier": "^2.3.2",
		"pretty-quick": "^3.1.1",
		"react-app-rewired": "^2.1.8"
	},
	"scripts": {
		"proxy": "lcp --proxyUrl https://ptr.gisat.cz --port 4444 --origin http://localhost:3000 --proxyPartial \"\" --credentials",
		"proxy:ssr": "lcp --proxyUrl https://ptr.gisat.cz --port 4444 --origin http://localhost:3001 --proxyPartial \"\" --credentials",
		"start": "npm run proxy | react-app-rewired start",
		"start:ssr": "react-app-rewired start | npm run proxy:ssr | cra-universal start",
		"build:ssr": "cra-universal build",
		"predeploy": "npm run build",
		"deploy": "gh-pages -b gh-pages -d build",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"prettier:write": "prettier --write 'src/**/*.js' 'server/**/*.js'"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
